{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "new-york",
  "rsc": false,
  "tsx": true,
  "tailwind": {
    "config": "tailwind.config.mjs",
    "css": "src/styles/globals.css",
    "baseColor": "stone",
    "cssVariables": true,
    "prefix": ""
  },
  "aliases": {
    "components": "@/components",
    "utils": "@/lib/utils",
    "ui": "@/components/ui",
    "lib": "@/lib",
    "layouts": "@/layouts",
    "hooks": "@/hooks",
    "blocks": "@/blocks",
    "pages": "@/pages",
    "styles": "@/styles",
    "videos": "@/videos",
    "custom": "@/custom",
    "json": "@/json",
    "types": "@/types"
  },
  "iconLibrary": "lucide",
  "registries": {
    "@shadcnblocks": {
      "url": "https://shadcnblocks.com/r/{name}",
      "headers": {
        "Authorization": "Bearer ${SHADCNBLOCKS_API_KEY}"
      }
    }
  }
}
